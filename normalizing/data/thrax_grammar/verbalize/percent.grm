import '../utf8.grm' as utf;
import '../util.grm' as util;
import 'decimal.grm' as dec;


sym = Optimize[StringFile['percent.tsv', 'utf8', 'utf8']];
s = util.s;
percent =
  util.Delete["percent|"]
  dec.DECIMAL_MARKUP
  util.Delete[s "symbol:"]
  util.Insert[" "]
  sym
  util.Delete[s "|"]
;

export PERCENT = Optimize[percent];